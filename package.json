        "prettify": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@rollup/plugin-typescript": "^12.1.2",
        "@types/jquery": "^3.5.32",
        "@types/node": "^22.10.10",
        "babel-jest": "^29.7.0",
        "jest": "^29.7.0",
        "jquery": "^3.7.1",
        "prettier": "^3.4.2",
        "rollup": "^4.31.0",
        "sass": "^1.83.4",
        "tslib": "^2.8.1",
        "typescript": "^5.7.3"
    },
    "jest": {
        "transform": {
            "^.+\\.js$": "babel-jest"
        },
        "testEnvironment": "node",
        "moduleNameMapper": {
            "^(\\.{1,2}/.*)\\.js$": "$1"
        }
